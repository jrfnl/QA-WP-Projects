{
    "name" : "jrfnl/qawpprojects",
    "description" : "A set of PHP Codesniffer standards which can be used to check the code quality of WordPress projects.",
    "keywords"   : [ "wordpress", "phpcs", "standards" ],
    "type"       : "phpcodesniffer-standard",
    "license"    : "MIT",
    "authors"    : [
        {
            "name"    : "Contributors",
            "homepage": "https://github.com/jrfnl/QA-WP-Projects/graphs/contributors"
        }
    ],
    "require"    : {
        "php" : ">=5.4",
        "squizlabs/php_codesniffer": "^3.5.2",
        "wp-coding-standards/wpcs": "^2.2.0",
        "phpcompatibility/phpcompatibility-wp": "^2.1.0",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.5.0",
        "wptrt/wpthemereview": "^0.2"
    },
    "minimum-stability" : "RC",
    "prefer-stable": true,
    "support"    : {
        "issues": "https://github.com/jrfnl/QA-WP-Projects/issues",
        "source": "https://github.com/jrfnl/QA-WP-Projects"
    }
}
